import java.util.*;
public class Elephant {
    public static void main(String[] args) {
        Scanner sc=new Scanner (System.in);
        int x=sc.nextInt();
        if(x<=5)System.out.println(1);
        else {
            int ans=(int)Math.ceil(x*1.0/5);
            System.out.println(ans);
        }
    }
}
