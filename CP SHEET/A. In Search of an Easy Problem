import java.util.*;
public class Insearchof {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
        for(int x:arr){
            if(x==1){
                System.out.println("HARD");
                return;
            }
        }
        System.out.println("EASY");
    }
}
